@Library('mySharedLibrary') _

pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                script {
                    def config = [
                        url: 'https://github.com/kokondakelvinjames/cicd_jenkinspipeline_final.git',
                        credentialsId: 'github_token',
                        branch: 'main'

                    ]
                    gitCheckout(config)
                }
                
            }
        }
        stage('SonarScan') {
            steps {
                script {
                     withSonarQubeEnv('sonar_server') {
                    sh '''
                        sonar-scanner -v
                        sonar-scanner
                    '''
                     }
                }
            }
        }


        stage('build') {
            steps {
                sh 'ls -lrt'
            }
        }

    }
}